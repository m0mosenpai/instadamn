<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="?attr/igds_color_primary_text"
                android:dashWidth="5dp"
                android:dashGap="5dp"/>
        </shape>
    </item>
    <item
        android:left="20dp"
        android:right="20dp">
        <shape android:shape="line">
            <stroke
                android:width="2dp"
                android:color="?attr/igds_color_primary_text"/>
        </shape>
    </item>
    <item
        android:left="20dp"
        android:right="20dp">
        <rotate
            android:fromDegrees="90"
            android:toDegrees="-90"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="?attr/igds_color_primary_text"/>
            </shape>
        </rotate>
    </item>
</layer-list>
