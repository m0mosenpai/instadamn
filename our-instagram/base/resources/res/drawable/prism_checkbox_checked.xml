<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:height="24dp"
                android:width="24dp"/>
            <corners android:radius="3dp"/>
            <solid android:color="@color/checkbox_background_color_selector"/>
        </shape>
    </item>
    <item
        android:gravity="center"
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp">
        <bitmap
            android:src="@drawable/instagram_check_pano_filled_16"
            android:tint="@color/checkbox_image_tint"/>
    </item>
</layer-list>
