<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="@dimen/add_account_icon_circle_radius"
                android:width="@dimen/add_account_icon_circle_radius"/>
            <solid android:color="?attr/igds_color_primary_background"/>
            <stroke
                android:width="@dimen/afi_indicator_arrow_margin_top"
                android:color="?attr/igds_color_stroke"/>
        </shape>
    </item>
    <item>
        <bitmap
            android:gravity="center"
            android:src="@drawable/instagram_unlock_pano_filled_16"
            android:tint="?attr/igds_color_stroke"/>
    </item>
</layer-list>
