<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="?attr/igds_color_secondary_text"/>
            <size
                android:height="@dimen/ab_test_media_thumbnail_preview_item_internal_padding"
                android:width="@dimen/ab_test_media_thumbnail_preview_item_internal_padding"/>
        </shape>
    </item>
    <item
        android:gravity="center"
        android:id="@+id/icon"
        android:left="@dimen/abc_action_bar_elevation_material"
        android:top="@dimen/abc_action_bar_elevation_material"
        android:right="@dimen/abc_action_bar_elevation_material"
        android:bottom="@dimen/abc_action_bar_elevation_material">
        <bitmap
            android:src="@drawable/instagram_x_pano_outline_12"
            android:tint="?attr/igds_color_primary_background"/>
    </item>
</layer-list>
