<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="top|left">
        <rotate
            android:fromDegrees="35"
            android:toDegrees="35"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape>
                <size
                    android:height="9dp"
                    android:width="2dp"/>
                <solid android:color="@color/grey_9"/>
                <corners android:radius="1dp"/>
            </shape>
        </rotate>
    </item>
    <item android:top="6.2dp">
        <rotate
            android:fromDegrees="-35"
            android:toDegrees="-35"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape>
                <size
                    android:height="9dp"
                    android:width="2dp"/>
                <solid android:color="@color/grey_9"/>
                <corners android:radius="1dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>
