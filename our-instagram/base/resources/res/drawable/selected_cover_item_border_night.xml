<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/fds_transparent"/>
            <inset
                android:insetLeft="1dp"
                android:insetRight="1dp"
                android:insetTop="1dp"
                android:insetBottom="1dp"/>
            <stroke
                android:width="3dp"
                android:color="@color/grey_9"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/fds_transparent"/>
            <stroke
                android:width="1dp"
                android:color="@color/design_dark_default_color_on_background"/>
        </shape>
    </item>
</layer-list>
