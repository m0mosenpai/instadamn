<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/seek_bar_knob_outer_circle">
        <shape
            android:shape="ring"
            android:useLevel="false"
            android:innerRadius="10dp"
            android:thickness="@dimen/afi_indicator_arrow_margin_top">
            <solid android:color="@color/grey_9"/>
        </shape>
    </item>
    <item
        android:id="@+id/seek_bar_knob_inner_circle"
        android:left="11dp"
        android:top="11dp"
        android:right="11dp"
        android:bottom="11dp">
        <shape android:shape="oval">
            <solid android:color="@color/grey_9"/>
        </shape>
    </item>
</layer-list>
