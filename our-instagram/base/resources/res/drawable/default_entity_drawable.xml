    <item
        android:gravity="center"
        android:drawable="@drawable/default_entity_drawable__app_bitmap_0"
        android:left="@dimen/ab_test_media_thumbnail_preview_item_internal_padding"
        android:top="@dimen/ab_test_media_thumbnail_preview_item_internal_padding"
        android:right="@dimen/ab_test_media_thumbnail_preview_item_internal_padding"
        android:bottom="@dimen/ab_test_media_thumbnail_preview_item_internal_padding">
        <size
            android:height="@dimen/abc_dialog_padding_material"
            android:width="@dimen/abc_dialog_padding_material"/>
    </item>
</layer-list>
