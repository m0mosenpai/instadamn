<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/clips_gradient_redesign_color_4"/>
            <stroke
                android:width="2dp"
                android:color="@color/audio_bar_action_color_enabled"/>
        </shape>
    </item>
    <item
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp">
        <bitmap
            android:gravity="center"
            android:src="@drawable/instagram_crown_pano_filled_12"
            android:tint="@color/audio_bar_action_color_enabled"/>
    </item>
</layer-list>
